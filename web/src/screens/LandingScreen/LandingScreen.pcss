        &__icon {
          width:24px;
          margin-right:6px;
        }
        &__table {
          align-items: center;
          display: inline-block;
          flex-direction: column;
          height: 100%;
          justify-content: flex-start;
          min-width: 900px;
          overflow: auto;
          width: 100%;
      
          thead,
          tbody {
            align-items: flex-start;
            display: flex;
            flex-direction: column;
            height: 100%;
            justify-content: flex-start;
            width: 100%;
      
            tr {
              align-items: center;
              display: flex;
              flex-direction: row;
              height: 38px;
              justify-content: space-evenly;
              padding: 0 24px;
              width: 100%;
      
              th,
              td {
                align-items: center;
                color: var(--table-text-color);
                display: flex;
                flex-direction: row;
                font-weight: normal;
                height: 38px;
                justify-content: flex-end;
                min-width: 110px;
                width: 100%;
                font-weight:700;
                
                &:first-child {
                  color: var(--bright);
                  justify-content: flex-start;
                }
              }
      
              th {
                font-size: 12px;
                line-height: 12px;
              }
      
              td {
                font-size: 14px;
                line-height: 24px;
              }
            }
          }
      
          thead {
            height: 38px;
          }
      
          tbody {
            overflow-y: auto;
      
            tr {
              &:hover {
                background: var(--table-row-selector-color);
                cursor: pointer;
      
                td {
                  color: var(--primary-text-color);
                }
              }
            }
      
            .no-data {
              align-items: flex-start;
              color: var(--table-text-color);
              display: flex;
              flex-direction: row;
              font-size: 14px;
              justify-content: center;
              line-height: 24px;
              padding: 10px 0;
              width: 100%;
            }
          }
        }
      }
    }
  }

  &__platform-info {
    background: var(--primary-cta-color);
    display: flex;
    height: 150px;

    &__wrap {
      align-items: center;
      display: flex;
      flex: 1;
      flex-direction: row;
      justify-content: center;

      &__item {
        display: flex;
        flex-direction: column;
        justify-content: center;
        margin: 0 50px;

        span {
          color: var(--primary-text-color);
          font-style: normal;
          text-align: center;

          &:first-child {
            font-size: 60px;
            font-weight: 500;
            line-height: 72px;
            margin-bottom: 8px;
          }

          &:last-child {
            font-size: 16px;
            font-weight: normal;
            letter-spacing: 0.25px;
            line-height: 28px;
            opacity: 0.5;
          }
        }
      }
    }
  }

  &__register {
    background-image: url(assets/images/landing/TradingUIFullBG.png);
    background-position: right center;
    background-repeat: no-repeat;
    background-size: 30%;

    &-content {
      justify-content: center;
      margin-top: 0 !important;
    }

    &__item {
      align-items: flex-start;
      display: flex;
      flex-direction: column;
      justify-content: flex-start;
      max-width: 464px;

      h1,
      h2 {
        text-align: left;
      }

      h1 {
       /* margin-bottom: 36px;*/
      }

      h2 {
        margin-bottom: 36px;
      }
    }
  }

