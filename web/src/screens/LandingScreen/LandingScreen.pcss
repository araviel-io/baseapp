    &__wrap {
      align-items: center;
      display: flex;
      flex-direction: column;
      justify-content: flex-end;
      margin-top: 90px;

      &__title {
        align-items: center;
        display: flex;
        flex-direction: column;
        justify-content: center;
        margin-top: 60px;

        .landing-button {
          margin: 60px 0;
        }
      }

      .pg-highlight-table {
        display:flex;
        &__spacer {
          width:12px;
        }
        &__element {
          margin-bottom: 70px;
          -webkit-box-shadow: 0px 3px 13px -2px var(--shadow-color-level-1);
          box-shadow: 0px 3px 13px -2px var(--shadow-color-level-1);
          //border-radius: 12px;
        }
        &__icon {
          width:24px;
          margin-right:6px;
        }
        &__filter {
          align-items: center;
          background: var(--subheader-background-color);
          border-bottom: 0.5px solid var(--table-border-color);
          display: flex;
          flex-direction: row;
          height: 57px;
          justify-content: flex-start;
          overflow: hidden;
          padding: 0 25px;
          width: 100%;
      
          ul {
            list-style-type: none;
            margin: 0;
            margin-block-end: 0;
            margin-block-start: 0;
            margin-inline-end: 0;
            margin-inline-start: 0;
            overflow: auto;
            padding-inline-start: 0;
          }
      
          .navigation {
            align-items: center;
            display: flex;
            flex-direction: row;
            height: 100%;
            justify-content: flex-start;
            overflow-x: auto;
            width: 100%;
      
            &::-webkit-scrollbar {
              height: 0;
            }
      
            &__item {
              align-items: center;
              cursor: pointer;
              display: flex;
              flex-direction: row;
              height: 100%;
              justify-content: center;
              min-width: 54px;
              padding-top: 0;
              width: 54px;
      
              &__link {
                color: var(--secondary-contrast-cta-color);
                font-size: 12px;
                line-height: 12px;
                text-decoration: none;
                transition: 0.2s linear;
              }
      
              &:hover > .navigation__item__link {
                color: var(--primary-text-color);
                transition: 0.2s;
              }
      
              &--active {
                border-bottom: 2px solid var(--primary-cta-color);
                padding-top: 2px;
      
                .navigation__item__link {
                  color: var(--primary-text-color);
                }
              }
            }
          }
        }
      
        &__table-wrap {
          align-items: center;
          display: block;
          flex-direction: column;
          height: 100%;
          justify-content: flex-start;
          width: 100%;
        }
      
        &__table {
          align-items: center;
          display: inline-block;
          flex-direction: column;
          height: 100%;
          justify-content: flex-start;
          width: 100%;
      
          thead,
          tbody {
            align-items: flex-start;
            display: flex;
            flex-direction: column;
            height: 100%;
            justify-content: flex-start;
            width: 100%;
      
            tr {
              align-items: center;
              display: flex;
              flex-direction: row;
              height: 38px;
              justify-content: space-evenly;
              padding: 0 24px;
              width: 100%;
              background: #f7f7f7;
              th,
              td {
                align-items: center;
                color: var(--table-text-color);
                display: flex;
                flex-direction: row;
                font-weight: normal;
                height: 38px;
                justify-content: flex-end;
                min-width: 110px;
                width: 100%;
                font-weight: 700;
                &:first-child {
                  color: var(--bright);
                  justify-content: flex-start;
                }
              }
      
              th {
                font-size: 12px;
                line-height: 12px;
              }
      
              td {
                font-size: 14px;
                line-height: 24px;
              }
            }
          }
      
          thead {
            height: 38px;
          }
      
          tbody {
         
      
            tr {
              &:hover {
                background: var(--table-row-selector-color);
                cursor: pointer;
      
                td {
                  color: var(--primary-text-color);
                }
              }
            }
      
            .no-data {
              align-items: flex-start;
              color: var(--table-text-color);
              display: flex;
              flex-direction: row;
              font-size: 14px;
              justify-content: center;
              line-height: 24px;
              padding: 10px 0;
              width: 100%;
            }
          }
        }
      }
      .pg-ticker-table {
        margin-bottom: 70px;
        -webkit-box-shadow: 0px 3px 13px -2px var(--shadow-color-level-1);
        box-shadow: 0px 3px 13px -2px var(--shadow-color-level-1);
