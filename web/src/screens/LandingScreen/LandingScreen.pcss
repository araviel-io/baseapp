.landing-block {
  &__content {
    max-width: 1310px !important;
  }
}

.pg-landing-screen {
  padding-top: 0 !important;

  .positive {
    color: var(--system-green);
  }

  .negative {
    color: var(--system-red);
  }

  h1 {
    align-items: center;
    font-family:'SafePoppins';
    color: var(--custom-safe);
    display: block;
    font-size: xxx-large;
    font-style: normal;
    font-weight: 900;
    letter-spacing: -0.02em;
    /*line-height: 56px;*/
    margin: 0;
    margin-block-end: 0;
    margin-block-start: 0;
    margin-inline-end: 0;
    margin-inline-start: 0;
    text-align: center;
  }
  h2 {
    align-items: center;
    color: var(--primary-text-color);
    display: flex;
    font-size: 23px;
    font-style: normal;
    font-weight: 500;
    margin: 0;
    margin-block-end: 0;
    margin-block-start: 0;
    margin-inline-end: 0;
    margin-inline-start: 0;
    text-align: center;
  }
  h3 {
    align-items: center;
    color: var(--primary-text-color);
    display: flex;
    font-size: 16px;
    font-style: normal;
    font-weight: normal;
    margin: 0;
    margin-block-end: 0;
    margin-block-start: 0;
    margin-inline-end: 0;
    margin-inline-start: 0;
    text-align: center;
  }
  p {
    font-size: 1.6rem;
    line-height: 1.7em;
    font-family: IBM Plex Sans,-apple-system,BlinkMacSystemFont,Segoe UI,Roboto,sans-serif;
  }
  .landing-button {
    align-items: center;
    background: var(--button-primary-cta-background-color);
    border-radius: 50px;
    color: var(--button-primary-cta-text-color);
    cursor: pointer;
    display: flex;
    flex-direction: row;
    font-size: 14px;
    font-style: normal;
    font-weight: normal;
    height: 44px;
    justify-content: center;
    letter-spacing: 1px;
    line-height: 16px;
    text-align: center;
    text-decoration: none;
    text-transform: uppercase;
    user-select: none;
    width: 270px;
    transition: all 0.2s ease 0s;
    &:hover {
      background-color: #2EE59D;
      box-shadow: 0px 15px 20px rgba(46, 229, 157, 0.4);
      color: #fff;
      transform: translateY(-5px);
    }
    &--secondary {
      background: var(--secondary-contrast-cta-color);
    }
  }

  .inverted {
    background: var(--button-primary-cta-text-color);
    border-radius: 50px;
    color: var(--button-primary-cta-background-color);
    
  }
  &__header,
  &__start-trading,
  &__footer {
    align-items: center;
    display: flex;
    flex-direction: column;
    height: 100%;
    justify-content: center;
    width: 100%;

    &__wrap {
      align-items: center;
      display: flex;
      flex-direction: column;
      height: 100%;
      justify-content: center;
      max-width: 1140px;
      width: 100%;
    }
  }

  &__header {
    //background: var(--subheader-background-color);
    height: 64px;
    position: fixed;
    top: 0;
    z-index: 99;
    
    &__wrap {
      flex-direction: row;
      justify-content: space-between;
      max-width: 1180px;
      padding: 0 20px;

      &__left img {
        max-height: 64px;
        max-width: 174px;
        object-fit: contain;
      }

      &__right {
        align-items: center;
        display: flex;
        flex-direction: row;
        justify-content: flex-end;

        .landing-button {
          height: 34px;
          width: 170px;

          &--simple {
            background: transparent;
            color: var(--button-contrast-cta-text-color);
            height: 32px;
            margin-right: 21px;
            width: 76px;

            &:hover {
              background: transparent;
              box-shadow: inherit;
              color: var(--custom-safe);
              transform: inherit;
            }
          }
        }
      }
    }
  }

  &__top {
    background: linear-gradient(0deg, rgba(0,151,205,0) 0%, rgba(0,115,158,0.10127801120448177) 100%);
    /*background: url(assets/images/landing/GridMainBG.svg);
    background-color: var(--landing-background-color);*/
    background-position: right center;
    background-repeat: no-repeat;
    background-size: unset;
    display: flex;
    padding-left: 0 !important;
    padding-right: 0 !important;

    &-content {
      flex-direction: column;
      margin: 0 !important;
      max-width: none !important;
    }
  }

  &__market-info {
    align-items: flex-start;
    display: flex;
    flex: 1;
    justify-content: center;
    background-repeat-y: no-repeat;
    background-position-y: bottom;
    background-size: contain;
