      color: var(--text-contrast-color);
      font-size: xxx-large;
      font-style: normal;
      font-weight: 900;
      letter-spacing: -0.02em;
      margin-bottom: 25px;
      text-align:center;
    }

    &__content {
      align-items: flex-start;
      display: flex;
      flex-direction: column;
      height: max-content;
      justify-content: flex-start;
      width: 100%;

      &__row {
        align-items: flex-start;
        display: flex;
        flex-direction: row;
        height: max-content;
        justify-content: space-between;
        width: 100%;

        &__item {
          align-items: flex-start;
          /*background: var(--landing-background-color);
